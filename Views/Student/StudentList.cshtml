@using Student015.Models 
@model List<Student>
<html>
<head>
    <link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.min.css" />
</head>
<body>
    @*--------Food list Table Start-------*@
    <div class="container">
        <center><h1> List Of Students </h1></center>
        <table class="table">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Name</th>
                    <th scope="col">Course</th>
                    <th scope="col">Semester</th>
                    <th scope="col">Birth Year</th>
                </tr>
            </thead> @for (int i = 0; i < Model.Count; i++)
            {
                <tr>
                    <th scope="row">@Model[i].Id</th>
                    <td>@Model[i].Name</td>
                    <td>@Model[i].Course</td>
                    <td>@Model[i].Semester</td>
                    <td>@Model[i].BirthYear</td>
                </tr>
            }
        </table>
    </div> @*--------Food List Table End-------*@
    <script src="/lib/jquery/dist/jquery.min.js"></script>
    <script src="/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>